            <stringProp name="BeanShellAssertion.query">// Variables for payload validation
// csv_x = from csv; res_x = from response
String csv_desc = vars.get(&quot;Desc1&quot;);
String res_desc = vars.get(&quot;Desc2&quot;);
String csv_id = vars.get(&quot;Id1&quot;);
String res_id = vars.get(&quot;Id2&quot;);
String csv_name = vars.get(&quot;Name1&quot;);
String res_name = vars.get(&quot;Name2&quot;);

//Variables for Response headers
String content_type = vars.get(&quot;contenttype&quot;);
String charset = vars.get(&quot;charset&quot;);
String server = vars.get(&quot;Server&quot;);
String char_control = vars.get(&quot;cachecontrol&quot;);
String exp = vars.get(&quot;expires&quot;);
String pragma = vars.get(&quot;pragma&quot;);
String pow_by = vars.get(&quot;xpoweredby&quot;);
String asp_ver = vars.get(&quot;aspnetver&quot;);
String post_session = vars.get(&quot;sessionid&quot;);
String get_session = vars.get(&quot;sessionid2&quot;);
String response = vars.get(&quot;response&quot;);

Failure = !csv_desc.equals(res_desc) || !csv_id.equals(res_id) || !csv_name.equals(res_name) || !content_type.equals(&quot;application/json&quot;) || !charset.equals(&quot;utf-8&quot;) || !server.equals(&quot;Microsoft-IIS/10.0&quot;) || !char_control.equals(&quot;no-cache&quot;) || !exp.equals(&quot;-1&quot;) || !pragma.equals(&quot;no-cache&quot;) || !pow_by.equals(&quot;4.0.30319&quot;) || !asp_ver.equals(&quot;ASP.NET&quot;) || !post_session.equals(get_session) || !response.equals(&quot;200 OK&quot;);

if (Failure) {
  FailureMessage = &quot;Response header or payload are not meeting the requirement&quot;;
}

</stringProp>
            <stringProp name="BeanShellAssertion.filename"></stringProp>
